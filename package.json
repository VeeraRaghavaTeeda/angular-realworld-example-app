
{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Installation: npm install\r Run the app: npm start",
  "dependencies": {
    "concurrently": "^6.2.1",
    "lru-cache": "^7.14.0"
  },
  "devDependencies": {
    "husky": "7.0.2",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0"
  },
  "scripts": {
    "format": "prettier --config ./.prettierrc --write \"./**/*.{js,jsx,ts,tsx,md,html,css,scss,json}\" ",
    "prepare": "husky install",
    "root-install": "cd root-module && npm install",
    "root": "cd root-module && npm start",
    "cms-install": "cd core-modules/cms && npm install",
    "cms": "cd core-modules/cms && npm start",
    "livedoc-install": "cd core-modules/livedoc-ts && npm install",
    "livedoc": "cd core-modules/livedoc-ts && npm start",
    "utils-install": "cd core-modules/utils && npm install",
    "utils": "cd core-modules/utils && npm start",
    "sports-install": "cd micro-applications/sports && npm install",
    "sports": "cd micro-applications/sports && npm start",
    "components-library-install": "cd core-modules/components-library && npm install",
    "components-library": "cd core-modules/components-library && npm start",
    "single-event-install": "cd micro-applications/single-event && npm install",
    "single-event": "cd micro-applications/single-event && npm start",
    "betting-app-install": "cd micro-applications/betting-app && npm install",
    "betting-app": "cd micro-applications/betting-app && npm start",
    "footer-install": "cd micro-applications/footer && npm install",
    "footer": "cd micro-applications/footer && npm start",
    "bethistory-app-install": "cd micro-applications/bethistory-app && npm install",
    "bethistory-app": "cd micro-applications/bethistory-app && npm start",
    "live-inplay-app-install": "cd micro-applications/live-inplay-app && npm install",
    "live-inplay-app": "cd micro-applications/live-inplay-app && npm start",
    "home-app-install": "cd micro-applications/home-app && npm install",
    "home-app": "cd micro-applications/home-app && npm start",
    "coupons-install": "cd micro-applications/coupons && npm install",
    "coupons": "cd micro-applications/coupons && npm start",
    "top-bets-app-install": "cd micro-applications/top-bets-app && npm install",
    "top-bets-app": "cd micro-applications/top-bets-app && npm start",
    "layout-app-install": "cd micro-applications/layout-app && npm install",
    "layout-app": "cd micro-applications/layout-app && npm start",
    "layout-left-install": "cd micro-applications/layout-left && npm install",
    "layout-left": "cd micro-applications/layout-left && npm start",
    "layout-right-install": "cd micro-applications/layout-right && npm install",
    "layout-right": "cd micro-applications/layout-right && npm start",
    "race-sports-install": "cd micro-applications/race-sports && npm install",
    "race-sports": "cd micro-applications/race-sports && npm start",
    "install:root-module": "concurrently \"npm run root-install\"",
    "install:core-module": "concurrently \"npm run cms-install\" \"npm run components-library-install\" \"npm run livedoc-install\" \"npm run utils-install\"",
    "install:app-module": "concurrently \"npm run sports-install\" \"npm run single-event-install\" \"npm run betting-app-install\" \"npm run footer-install\" \"npm run bethistory-app-install\" \"npm run live-inplay-app-install\" \"npm run home-app-install\" \"npm run top-bets-app-install\" \"npm run coupons-install\" \"npm run layout-app-install\" \"npm run layout-left-install\" \"npm run layout-right-install\" \"npm run race-sports-install\"",
    "start": "concurrently \"npm run root\" \"npm run cms\" \"npm run livedoc\" \"npm run utils\" \"npm run sports\" \"npm run components-library\" \"npm run single-event\" \"npm run betting-app\" \"npm run footer\" \"npm run bethistory-app\" \"npm run live-inplay-app\" \"npm run home-app\" \"npm run top-bets-app\" \"npm run coupons\" \"npm run layout-app\" \"npm run layout-left\" \"npm run layout-right\" \"npm run race-sports\"",
    "root-build": "cd root-module && npm run build",
    "cms-build-dev-pp": "cd core-modules/cms && npm run build:dev:pp",
    "cms-build-uat-pp": "cd core-modules/cms && npm run build:uat:pp",
    "cms-build-prod-pp": "cd core-modules/cms && npm run build:prod:pp",
    "cms-build-dev-nitro": "cd core-modules/cms && npm run build:dev:nitro",
    "cms-build-uat-nitro": "cd core-modules/cms && npm run build:uat:nitro",
    "cms-build-prod-nitro": "cd core-modules/cms && npm run build:prod:nitro",
    "cms-build-dev-dazn": "cd core-modules/cms && npm run build:dev:dazn",
    "cms-build-uat-dazn": "cd core-modules/cms && npm run build:uat:dazn",
    "cms-build-prod-dazn": "cd core-modules/cms && npm run build:prod:dazn",
    "livedoc-build": "cd core-modules/livedoc-ts && npm run build",
    "utils-build": "cd core-modules/utils && npm run build",
    "sports-build": "cd micro-applications/sports && npm run build:webpack",
    "components-library-build": "cd core-modules/components-library && npm run build:webpack",
    "single-event-build": "cd micro-applications/single-event && npm run build:webpack",
    "betting-app-build": "cd micro-applications/betting-app && npm run build:webpack",
    "footer-build": "cd micro-applications/footer && npm run build:webpack",
    "bethistory-app-build": "cd micro-applications/bethistory-app && npm run build",
    "live-inplay-app-build": "cd micro-applications/live-inplay-app && npm run build:webpack",
    "home-app-build": "cd micro-applications/home-app && npm run build:webpack",
    "coupons-build": "cd micro-applications/coupons && npm run build:webpack",
    "top-bets-app-build": "cd micro-applications/top-bets-app && npm run build:webpack",
    "layout-app-build": "cd micro-applications/layout-app && npm run build:webpack",
    "layout-left-build": "cd micro-applications/layout-left && npm run build:webpack",
    "layout-right-build": "cd micro-applications/layout-right && npm run build:webpack",
    "race-sports-build": "cd micro-applications/race-sports && npm run build:webpack",
    "build:root-module": "npm run root-build",
    "build:core-module": "npm run components-library-build && npm run livedoc-build  && npm run utils-build",
    "build:app-module": "npm run sports-build && npm run single-event-build && npm run betting-app-build && npm run footer-build && npm run bethistory-app-build && npm run live-inplay-app-build && npm run home-app-build && npm run top-bets-app-build && npm run coupons-build && npm run race-sports-build",
    "build:layout-app": "npm run layout-app-build && npm run layout-left-build && npm run layout-right-build",
    "build:package": "npm run build:layout-app && npm run build:root-module && npm run build:core-module && npm run build:app-module"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.gametechlabs.net/ppbet/frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,scss,json}": "prettier --write"
  }
}
